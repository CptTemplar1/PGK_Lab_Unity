    <RmHLSLShader NAME="Vertex Shader" API="D3D" PIXEL_SHADER="FALSE" TARGET="vs_3_0" ENTRY_POINT="vs_main" COMPILER_FLAGS="0"><RmShaderCode TYPE="TEXT"><![CDATA[
float3 fvLightPosition;
float3 fvEyePosition;
float4 fvViewPosition;
float4x4 matView;
float4x4 matProjection;

float3 fvUp;
bool bAnimateSnowCover;
float fSnowFalloffPower;
float fSnowCoverValue;
float fSnowCoverHeight;
float fTerrainScale;
float3 fvWindDirection;
float fSnowCoverWindBias;

float fTime0_1;
float fNearClipPlane;
float fFarClipPlane;

static float fSnowProgress = 1.0f;

struct VS_INPUT 
{
	float4 Position : POSITION0;
	float2 Texcoord : TEXCOORD0;
	float3 Normal :   NORMAL0;
	float3 Binormal : BINORMAL0;
	float3 Tangent :  TANGENT0;
};

struct VS_OUTPUT 
{
	float4 Position :        POSITION0;
	float2 Texcoord :        TEXCOORD0;
	float3 ViewDirection :   TEXCOORD1;
	float3 LightDirection:   TEXCOORD2;
	float3 PositionLocal:    TEXCOORD3;
	float3 PositionGlobal:   TEXCOORD4;
	float  SnowFactor:       TEXCOORD5;
	float  Z:                TEXCOORD6;
};

float getSnowFactor(float3 _normal)
{
	float fSnow = fSnowProgress * fSnowCoverValue;
	float result = dot(normalize(_normal), normalize(fvUp));
	result += dot(normalize(_normal), normalize(fvWindDirection)) * fSnowCoverWindBias;
	result = clamp(result, fSnow, 1.0f - fSnow);
	result -= fSnow;
	result = result / (1.0f - 2.0f * fSnow);
	 
	return pow(result, fSnowFalloffPower);
}

VS_OUTPUT vs_main(VS_INPUT Input)
{
	if (bAnimateSnowCover)
		fSnowProgress = fTime0_1;
	
	VS_OUTPUT Output;
	
	Input.Position.xyz *= fTerrainScale;
	
	Output.SnowFactor       = getSnowFactor(Input.Normal);
	Output.Position         = Input.Position + float4(Input.Normal, 0.0f) * 
		Output.SnowFactor * fSnowCoverHeight * fSnowProgress;
	Output.Position         = mul(Output.Position, mul(matView, matProjection));
	Output.Texcoord         = Input.Texcoord;
	
	float3 fvObjectPosition = mul(Input.Position, matView);
	
	float3 fvViewDirection  = fvViewPosition - fvObjectPosition;
	float3 fvLightDirection = fvLightPosition - fvObjectPosition;
 	
	float3 fvNormal         = mul(Input.Normal, matView);
	float3 fvBinormal       = mul(Input.Binormal, matView);
	float3 fvTangent        = mul(Input.Tangent, matView);
  	
	Output.ViewDirection.x  = dot(fvTangent, fvViewDirection);
	Output.ViewDirection.y  = dot(fvBinormal, fvViewDirection);
	Output.ViewDirection.z  = dot(fvNormal, fvViewDirection);
	
	Output.LightDirection.x  = dot(fvTangent, fvLightDirection);
	Output.LightDirection.y  = dot(fvBinormal, fvLightDirection);
	Output.LightDirection.z  = dot(fvNormal, fvLightDirection);
	
	// Clouds relative to terrain, position not transformed
	Output.PositionLocal = Input.Position.xyz;
	Output.PositionGlobal = Output.Position.xyz;
	
	Output.Z = (Output.Position.z - fNearClipPlane) / (fFarClipPlane - fNearClipPlane);
	
	return Output;
}]]></RmShaderCode>
