     <RmSampler NAME="snowflakeMap" API="" REGISTER="0"/>
     <RmShaderConstant NAME="fvAmbient" API="" REGISTER="3"/>
     <RmShaderConstant NAME="fvCloudSize" API="" REGISTER="1"/>
     <RmShaderConstant NAME="fvSnowflakeTiles" API="" REGISTER="0"/>
