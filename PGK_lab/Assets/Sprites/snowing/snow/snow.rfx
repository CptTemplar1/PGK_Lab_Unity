    <RmHLSLShader NAME="Vertex Shader" API="D3D" PIXEL_SHADER="FALSE" TARGET="vs_3_0" ENTRY_POINT="vs_main" COMPILER_FLAGS="0"><RmShaderCode TYPE="TEXT"><![CDATA[
float4x4 matView;
float4x4 matProjection;

float3 fvSnowAreaCenter;
float3 fvSnowAreaSize;
float4 fvSnowflakeSize;
float fSnowFallSpeed;
float fSnowFallSpeedDelta;
float3 fvWindDirection;
float fSnowWindBias;
float fSnowWindBiasPower;
float fWindGustStrength;
float fWindGustFrequency;

float3 fvUp;
float fTime0_1;
float fNearClipPlane;
float fFarClipPlane;

int iParticleCount;

struct VS_INPUT 
{
    float4 Position : POSITION0;
    float2 TexCoord : TEXCOORD0;
};

struct VS_OUTPUT 
{
    float4 Position :     POSITION0;
    float2 TexCoord :     TEXCOORD0;
    float PartId:         TEXCOORD1;
    float3 PositionLocal: TEXCOORD2;
    float Z:              TEXCOORD3;
};

static float random_counter = 0.0f;
// Random number generator by Michael Pohoreski, thanks!
float random(float2 p)
{
	p += random_counter;
	random_counter += 1.0f;
	const float2 r = float2(
		23.1406926327792690,  // e^pi (Gelfond's constant)
    	2.6651441426902251);  // 2^sqrt(2) (Gelfond–Schneider constant)
	return frac(cos(fmod(123456789., 1e-7 + 256. * dot(p,r))));  
}

VS_OUTPUT vs_main(VS_INPUT In)
{
	VS_OUTPUT Out;
	
	float fId = In.Position.z / iParticleCount;
	float4 fvPos = float4(fvSnowAreaCenter.xyz, 1.0f);
	
	float3 fvRelPos = 0.0f;
	fvRelPos.x = random(fId);
	fvRelPos.y = random(fId);
	fvRelPos.z = random(fId);
	
	fvRelPos -= normalize(fvUp) * fSnowFallSpeed * 
		(1.0f + 2.0f * fSnowFallSpeedDelta * random(fId) - fSnowFallSpeedDelta) 
		* fTime0_1;
	fvRelPos -= fvWindDirection * (1.0f + 2.0f * fSnowFallSpeedDelta * random(fId) 
		- fSnowFallSpeedDelta) * fSnowWindBias * pow(1.0f - frac(fvRelPos.y), 
		fSnowWindBiasPower);
	float fX = fTime0_1 * fWindGustFrequency * 3.1415269f;
	fvRelPos -= fSnowWindBias * fWindGustStrength * fvWindDirection 
		* (fX - sin(fX) * cos(fX)) / fWindGustFrequency;
	
	fvRelPos = frac(fvRelPos);
	
	fvPos.xyz += (fvRelPos - 0.5f) * fvSnowAreaSize;
	
	float4 fvViewPos = mul(fvPos, matView);
	float2 fvSize = fvSnowflakeSize.xy;
	fvSize += random(fId) * (fvSnowflakeSize.zw - fvSnowflakeSize.xy);
	fvViewPos.xy += fvSize * 2.0f * In.TexCoord.xy - 1.0f;
	
	Out.Position = mul(fvViewPos, matProjection);
	Out.TexCoord = In.TexCoord;
	Out.PartId = In.Position.z;
	Out.PositionLocal = fvPos.xyz;
