    <RmHLSLShader NAME="Pixel Shader" API="D3D" PIXEL_SHADER="TRUE" TARGET="ps_3_0" ENTRY_POINT="ps_main" COMPILER_FLAGS="0"><RmShaderCode TYPE="TEXT"><![CDATA[
sampler2D renderMap;

float fBlurRange;
int iBlurSamples;

struct PS_INPUT
{
	float2 TexCoord: TEXCOORD0;
};

float4 ps_main(PS_INPUT Input) : COLOR0
{   
	float4 fvColor = 0.0f;
	float fTotal = 0.0f;
	
	for (int x = 0; x < iBlurSamples; x++)
	{
		for (int y = 0; y < iBlurSamples; y++)
		{
			float2 fvCoord = Input.TexCoord;
			float2 fvDelta = float2(x, y);
			fvDelta -= fvDelta / 2.0f;
			fvDelta /= float2(iBlurSamples, iBlurSamples);
			fvColor += tex2D(renderMap, fvCoord + fvDelta * fBlurRange);
		}
	}
	
	fvColor /= iBlurSamples * iBlurSamples;
	
	return fvColor;
}



]]></RmShaderCode>
