    <RmHLSLShader NAME="Pixel Shader" API="D3D" PIXEL_SHADER="TRUE" TARGET="ps_3_0" ENTRY_POINT="ps_main" COMPILER_FLAGS="0"><RmShaderCode TYPE="TEXT"><![CDATA[
float2 fvSnowflakeTiles;
int iParticleCount;
float2 fvCloudSize;
float3 fvWindDirection;
float4 fvAmbient;

float fTime0_1;

sampler2D snowflakeMap;
sampler2D cloudMap;

struct PS_INPUT
{
	float2 TexCoord : TEXCOORD0;
    float PartId: TEXCOORD1;
    float3 PositionLocal: TEXCOORD2;
    float Z: TEXCOORD3;
};

struct PS_OUTPUT
{
	float4 Color0: COLOR0;
	float4 Color1: COLOR1;
};

PS_OUTPUT ps_main(PS_INPUT Input)
{
	float fFrame = Input.PartId;
	float fCol = fmod(fFrame, fvSnowflakeTiles.x);
	float fRow = floor(fFrame / fvSnowflakeTiles.x);
	
	float2 fvTexCoords = Input.TexCoord;
	fvTexCoords = frac(fvTexCoords);
	fvTexCoords.x = (fCol + frac(Input.TexCoord.x)) / fvSnowflakeTiles.x;
	fvTexCoords.y = (fRow + frac(Input.TexCoord.y)) / fvSnowflakeTiles.y;
	
	float4 fvColor = tex2D(snowflakeMap, fvTexCoords);
	
	float4 fvCloudColor = tex2D(cloudMap, (Input.PositionLocal.xz / fvCloudSize) 
		+ fvWindDirection.xz * fTime0_1);
	fvCloudColor.xyz = 1.0f - fvCloudColor.xyz;
	fvCloudColor.w = 1.0f;
	
	// Correction for inexplicable thin white borders around snowflakes.
	if (Input.TexCoord.x < 0.05 || Input.TexCoord.x > 0.95
		|| Input.TexCoord.y < 0.05 || Input.TexCoord.y > 0.95)
		fvColor.w = 0.0f;
	
	PS_OUTPUT Output = (PS_OUTPUT) 0;
	Output.Color0.xyz = saturate(fvColor * fvAmbient + (1.0f - float4(fvAmbient.xyz, 1.0f)) * fvCloudColor);
	Output.Color0.w = fvColor.w;
	Output.Color1 = float4(Input.Z, Input.Z, Input.Z, 1.0f);
	
	return Output;
}



]]></RmShaderCode>
