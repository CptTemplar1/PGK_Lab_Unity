    <RmHLSLShader NAME="Vertex Shader" API="D3D" PIXEL_SHADER="FALSE" TARGET="vs_3_0" ENTRY_POINT="vs_main" COMPILER_FLAGS="0"><RmShaderCode TYPE="TEXT"><![CDATA[
float4x4 matView;
float4x4 matProjection;

float3 fvWindDirection;

struct VS_OUTPUT
{
   float4 Position  : POSITION0;
   float2 TexCoord  : TEXCOORD0;
   float3 WindDir   : TEXCOORD1;
};

VS_OUTPUT vs_main(float4 inPos: POSITION)
{
   VS_OUTPUT Output = (VS_OUTPUT) 0;

   inPos.xy = sign(inPos.xy);
   Output.Position = float4(inPos.yx, 0.0f, 1.0f);

   // get into range [0,1]
   Output.TexCoord = (float2(Output.Position.x, -Output.Position.y) + 1.0f)/2.0f;
   Output.WindDir = mul(float4(fvWindDirection, 0.0f), mul(matView, matProjection)).xyz;
   return Output;
}
]]></RmShaderCode>
