    <RmHLSLShader NAME="Pixel Shader" API="D3D" PIXEL_SHADER="TRUE" TARGET="ps_3_0" ENTRY_POINT="ps_main" COMPILER_FLAGS="0"><RmShaderCode TYPE="TEXT"><![CDATA[
float4 fvFogColor;
float fFogStart;
float fFogEnd;
float fFogFalloff;

float fDOFValue;
float fDOFFocus;
float fDOFFocusRange;

float fAuroraIntensity;

float fNearClipPlane;
float fFarClipPlane;

sampler2D terrainMap;
sampler2D terrainZMap;
sampler2D snowMap;
sampler2D snowZMap;
sampler2D auroraMap;

const float2 fvaGauss[12] = {
   -0.326212, -0.405805,
   -0.840144, -0.073580,
   -0.695914,  0.457137,
   -0.203345,  0.620716,
    0.962340, -0.194983,
    0.473434, -0.480026,
    0.519456,  0.767022,
    0.185461, -0.893124,
    0.507431,  0.064425,
    0.896420,  0.412458,
   -0.321940, -0.932615,
   -0.791559, -0.597705
};

struct PS_INPUT
{
	float2 TexCoord: TEXCOORD0;
	float3 WindDir:  TEXCOORD1;
};

struct PointOutput
{
	float4 Color;
	float Z;
};

static float4 fvAuroraColor = 0.0f;

float getDistance(float _unitDist)
{
	float result = _unitDist * (fFarClipPlane - fNearClipPlane);
	result = clamp(result, fFogStart, fFogEnd);
	result -= fFogStart;
	result /= fFogEnd - fFogStart;
	result = 1.0f - saturate(result);
	return result;
}

PointOutput getPoint(in sampler2D _color, in sampler2D _z, in float2 _texCoord)
{
	float4 fvResult = tex2D(_color, _texCoord);
	float fZCenter = tex2D(_z, _texCoord).x;
	float fZ = fZCenter;
	
	for (int i = 0; i < 12; i++)
	{
		float2 fvCoord = _texCoord + fvaGauss[i] * fDOFValue
			* abs(fZCenter - fDOFFocus / fDOFFocusRange);
		fvCoord = saturate(fvCoord);
		float4 fvCol = tex2D(_color, fvCoord);
		fvResult += fvCol;
		fZ += tex2D(_z, fvCoord);
	}
	
	fvResult /= 13.0f;
	fZ /= 13.0f;
	fvResult.xyz = lerp(fvAuroraColor, fvResult, pow(getDistance(fZ), fFogFalloff)).xyz;
	
	PointOutput Result;
	Result.Color = fvResult;
	Result.Z = fZ;
	return Result;
}

float4 ps_main(PS_INPUT Input) : COLOR0
{
	//Aurora
	fvAuroraColor = tex2D(auroraMap, Input.TexCoord);
	fvAuroraColor = lerp(fvFogColor, fvAuroraColor, length(fvAuroraColor.xyz));
	float fAuroraInt = length(fvAuroraColor.xyz);
	fvAuroraColor = fvAuroraColor * fAuroraInt * fAuroraIntensity;
	
	PointOutput Terrain = getPoint(terrainMap, terrainZMap, Input.TexCoord);
	PointOutput Snow = getPoint(snowMap, snowZMap, Input.TexCoord);
	
	float4 fvColor = 0.0f;
	
	if (Terrain.Z > Snow.Z)
	{
		fvColor = lerp(Terrain.Color, Snow.Color, Snow.Color.w);
	}
	else
		fvColor = Terrain.Color;
	
	return float4(fvColor.xyz, 1.0f);
}



]]></RmShaderCode>
