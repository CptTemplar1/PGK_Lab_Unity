sampler2D snowNormalMap;
sampler2D cloudMap;

struct PS_INPUT 
{
	float2 Texcoord :        TEXCOORD0;
	float3 ViewDirection :   TEXCOORD1;
	float3 LightDirection:   TEXCOORD2;
	float3 PositionLocal:    TEXCOORD3;
	float3 PositionGlobal:   TEXCOORD4;
	float  SnowFactor:       TEXCOORD5;
	float  Z:                TEXCOORD6;
};

struct PS_OUTPUT
{
	float4 Color0: COLOR0;
	float4 Color1: COLOR1;
};

// Returns light diffuse component as x and specular component as y
float2 getLightValues(float3 _lightDir, float3 _viewDir, float4 _normalMapColor)
{
	float2 result;
	
	float3 fvNormal = normalize((_normalMapColor.xyz * 2.0f) - 1.0f);
	result.x = dot(fvNormal, _lightDir);
	
	float3 fvReflection = normalize(((2.0f * fvNormal) * result.x) - _lightDir); 
	float3 fvViewDirection = normalize(_viewDir);
	result.y = max(0.0f, dot(fvReflection, fvViewDirection));
	
	return result;
}

static float random_counter = 0.0f;
// Random number generator by Michael Pohoreski, thanks!
float random(float2 p)
{
	p += random_counter;
	random_counter += 1.0f;
  	const float2 r = float2(
    	23.1406926327792690,  // e^pi (Gelfond's constant)
     	2.6651441426902251);  // 2^sqrt(2) (Gelfond–Schneider constant)
  	return frac(cos(fmod(123456789., 1e-7 + 256. * dot(p,r))));  
}

PS_OUTPUT ps_main(PS_INPUT Input)
{
	float2 fvTexCoordBase = Input.Texcoord * fvBaseTiling;
	float2 fvTexCoordSnow = Input.Texcoord * fvSnowTiling;
	
	float3 fvLightDirection = normalize(Input.LightDirection);
	
	float2 fvLightingBase = getLightValues(fvLightDirection, 
		Input.ViewDirection, tex2D(normalMap, fvTexCoordBase));
	float2 fvLightingSnow = getLightValues(fvLightDirection, 
		Input.ViewDirection, tex2D(snowNormalMap, fvTexCoordSnow));
		
	// No specular on snow, just for testing.
	fvLightingSnow.y = random(floor((Input.PositionLocal.xz)
		* fvSnowGlitterScale) + random(fvViewPosition.xz + fvViewPosition.y));
	
	float4 fvBaseColor = tex2D(baseMap, fvTexCoordBase);
	float4 fvSnowColor = tex2D(snowMap, fvTexCoordSnow);
	float4 fvCloudColor = tex2D(cloudMap, (Input.PositionLocal.xz / fvCloudSize) 
		+ fvWindDirection.xz * fTime0_1);
	fvCloudColor.xyz = 1.0f - fvCloudColor.xyz;
	fvCloudColor.w = 1.0f;
	
	float4 fvCombinedColor = lerp(fvBaseColor, fvSnowColor, Input.SnowFactor);
	float2 fvCombinedLighting = lerp(fvLightingBase, fvLightingSnow, Input.SnowFactor);
	
	float4 fvTotalAmbient = fvAmbient * fvCombinedColor; 
	float4 fvTotalDiffuse = fvDiffuse * fvCombinedLighting.x * fvCombinedColor; 
	float4 fvTotalSpecular = fvSpecular * pow(fvCombinedLighting.y, fSpecularPower);
	
	PS_OUTPUT Output = (PS_OUTPUT) 0;
	Output.Color0 = float4(saturate(fvTotalAmbient + (fvTotalDiffuse + fvTotalSpecular) 
		* fvCloudColor).xyz, 1.0f);
	Output.Color1 = float4(Input.Z, Input.Z, Input.Z, 1.0f);
	
	return Output;
}

]]></RmShaderCode>
